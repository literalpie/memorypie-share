{
  "name": "memorypie-share",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#convex/*": "./convex/*.js",
    "#src/*": "./src/*.js"
  },
  "scripts": {
    "dev": "concurrently 'pnpm run dev:frontend' 'pnpm run dev:backend'",
    "dev:frontend": "vite --open",
    "dev:backend": "convex dev",
    "predev": "convex dev --until-success && convex dashboard",
    "build": "tsgo -b && vite build",
    "lint": "tsgo && oxlint && oxfmt --check",
    "preview": "vite preview"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.58.0",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-form": "^1.27.1",
    "@tanstack/react-router": "^1.139.14",
    "@tanstack/react-router-devtools": "^1.139.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.30.0",
    "lucide-react": "^0.556.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-dropzone": "^14.3.8",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/router-plugin": "^1.139.14",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript/native-preview": "7.0.0-dev.20251206.1",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "globals": "^16.5.0",
    "oxfmt": "^0.23.0",
    "oxlint": "^1.38.0",
    "oxlint-tsgolint": "^0.11.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "vite": "8.0.0-beta.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "@tailwindcss/oxide"
    ]
  }
}
